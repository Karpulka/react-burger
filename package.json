{
  "name": "react-burger",
  "version": "0.1.0",
  "homepage": "https://karpulka.github.io/react-burger",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@types/node": "16.18.2",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@ya.praktikum/react-developer-burger-ui-components": "^1.14.0",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.1",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "typescript": "4.8.4",
    "uuid": "^9.0.0",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/redux-mock-store": "^1.0.3",
    "cypress": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fetch-mock": "^9.11.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.3.1",
    "jest-watch-typeahead": "^0.6.5",
    "prettier": "^2.7.1",
    "react-dnd-cjs": "^9.5.1",
    "react-dnd-html5-backend-cjs": "^9.5.1",
    "redux-mock-store": "^1.5.4",
    "utility-types": "^3.10.0"
  },
  "jest": {
    "moduleNameMapper": {
      "react-dnd": "react-dnd-cjs",
      "react-dnd-html5-backend": "react-dnd-html5-backend-cjs",
      "dnd-core": "dnd-core-cjs"
    }
  }
}
